<Application x:Class="PriceSetterDesktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="PeekWindow.xaml">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <!--Colors and custom styles-->
        <ResourceDictionary Source="pack://application:,,,/PriceSetterDesktop;component/Resource/Styles/Colors.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/PriceSetterDesktop;component/Resource/Styles/Styles.xaml"/>
        
        <ResourceDictionary Source="pack://application:,,,/PriceSetterDesktop;component/Resource/ResourceBridge.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/WPFCollection;component/ResourceCollector.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
